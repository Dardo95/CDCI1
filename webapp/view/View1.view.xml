<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" xmlns:layout="sap.ui.layout"
	xmlns="sap.uxap" controllerName="CDCI1.controller.View1" displayBlock="true">
	<ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="false" showTitleInHeaderContent="false">
		<headerTitle>
			<ObjectPageHeader objectTitle="{i18n>title}">
				<actions>
					<layout:HorizontalLayout>
						<m:Text text="{i18n>usuario}:"/>
						<m:Text text="Nombre/apellidos"/>
					</layout:HorizontalLayout>
					<layout:HorizontalLayout>
							<m:Text text="{i18n>Fecha Actual}:"/>
							<m:Text text="hh:mm"/>
						</layout:HorizontalLayout>
				</actions>
			</ObjectPageHeader>
		</headerTitle>
		<headerContent>
			<layout:VerticalLayout>
				<layout:HorizontalLayout>
					<m:Text id="area" text="{i18n>area}:"/>
					<m:Text text="area"/>
					<m:Text text="{i18n>sociedad}:"/>
					<m:Text text="1000"/>
				</layout:HorizontalLayout>
			</layout:VerticalLayout>
		</headerContent>
	</ObjectPageLayout>
</mvc:View>