<mvc:View controllerName="CDCI1.controller.View1" height="100%" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core"
	xmlns:layout="sap.ui.layout" xmlns="sap.m" xmlns:f="sap.f" xmlns:t="sap.ui.table" xmlns:c="sap.ui.commons">
	<f:DynamicPage id="dynamicPageId" headerExpanded="{/headerExpanded}" toggleHeaderOnTitleClick="{/titleClickable}">
		<!-- DynamicPage Title -->
		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title text="{i18n>title}"/>
				</f:heading>
				<f:actions>
					<ToolbarSpacer/>
					<ToggleButton pressed="{/headerExpanded}" text="{path:'/headerExpanded', formatter:'.formatToggleButtonText'}" type="Transparent"/>
					<layout:VerticalLayout>
						<layout:HorizontalLayout>
							<Text text="{i18n>usuario}:"/>
							<Text text="Nombre/apellidos"/>
						</layout:HorizontalLayout>
						<layout:HorizontalLayout>
							<Text text="{i18n>fecha}:"/>
							<Text text="dd:mm:aa"/>
							<Text text="{i18n>hora}:"/>
							<Text text="hh:mm"/>
						</layout:HorizontalLayout>
					</layout:VerticalLayout>
				</f:actions>
			</f:DynamicPageTitle>
		</f:title>
		<f:header>
			<f:DynamicPageHeader pinnable="true">
				<f:content>
					<layout:Grid defaultSpan="XL4 L6 M6 S12">
						<layout:VerticalLayout>
							<Label text="Name:"/>
							<MultiComboBox selectionChange="handleSelectionChange" selectionFinish="handleSelectionFinish" width="200px" items="{vuelos>/FLISet}">
								<core:Item key="{vuelos>Carrid}" text="{vuelos>Carrid}"/>
							</MultiComboBox>
						</layout:VerticalLayout>
						<layout:VerticalLayout>
							<Label text="Name:"/>
							<Select id="slName4" width="200px" forceSelection="true" change="onSelectChange" items="{ path: '/ProductNames' }">
								<core:Item key="{key}" text="{name}"/>
							</Select>
						</layout:VerticalLayout>
						<layout:VerticalLayout>
							<Label text="Name:"/>
							<Select id="slName2" width="200px" forceSelection="true" change="onSelectChange" items="{ path: '/ProductNames' }">
								<core:Item key="{key}" text="{name}"/>
							</Select>
						</layout:VerticalLayout>
						<layout:VerticalLayout>
							<Label text="Name:"/>
							<Select id="slName3" width="200px" forceSelection="true" change="onSelectChange" items="{ path: '/ProductNames' }">
								<core:Item key="{key}" text="{name}"/>
							</Select>
						</layout:VerticalLayout>
					</layout:Grid>
				</f:content>
			</f:DynamicPageHeader>
		</f:header>
		<!-- DynamicPage Content -->
		<f:content>
			<t:Table rows="{vuelos>/FLIGHTSet}" title="Vuelos" selectionMode="None" visibleRowCount="250">
				<t:columns>
					<t:Column sorted="true">
						<!--<t:multiLabels>-->
							<!--<Label text="{i18n>listadenecesidades}"/>-->
							<Label text="Carrid"/>
						<!--</t:multiLabels>-->
						<t:template>
							<Text text="{vuelos>Carrid}"/>
						</t:template>
					</t:Column>
					<t:Column sorted="true">
						<t:multiLabels>
							<!--<Label text="{i18n>listadenecesidades}"/>-->
							<Label text="Connid"/>
						</t:multiLabels>
						<t:template>
							<Text text="{vuelos>Connid}"/>
						</t:template>
					</t:Column>
					<t:Column>
						<t:multiLabels>
							<Label text="{i18n>listadenecesidades}"/>
							<Label text="Fldate"/>
						</t:multiLabels>
						<t:template>
							<Text text="{vuelos>Fldate}"/>
						</t:template>
					</t:Column>
				</t:columns>
				<t:columns title="i18n>{acciones}">
					<t:Column>
						<t:multiLabels>
							<Label text="{i18n>acciones}"/>
							<Label text="{i18n>ver}"/>
						</t:multiLabels>
						<t:template>
							<Button type="Default" icon="sap-icon://show" press="getSelectedColIndex" width="100%"></Button>
						</t:template>
					</t:Column>
					<t:Column>
						<t:multiLabels>
							<Label text="{i18n>acciones}"/>
							<Label text="{i18n>aprobado}"/>
						</t:multiLabels>
						<t:template>
							<Button type="Accept" icon="sap-icon://thumb-up" press="getSelectedColIndex" width="100%"></Button>
						</t:template>
					</t:Column>
					<t:Column>
						<t:multiLabels>
							<Label text="{i18n>acciones}"/>
							<Label text="{i18n>rechazado}"/>
						</t:multiLabels>
						<t:template>
							<core:Icon src="sap-icon://thumb-down" class="size2" color="red"/>
						</t:template>
					</t:Column>
				</t:columns>
			</t:Table>
		</f:content>
	</f:DynamicPage>
